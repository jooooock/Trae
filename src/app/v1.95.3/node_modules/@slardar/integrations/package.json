{
  "name": "@slardar/integrations",
  "version": "2.8.4",
  "description": "Slardar Integrations",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": ["dist", "esm", "CHANGELOG.md"],
  "publishConfig": {
    "registry": "http://bnpm.byted.org/"
  },
  "devDependencies": {
    "@slardar/rollup": "0.0.3",
    "fake-indexeddb": "^4.0.1",
    "idb-keyval": "^6.2.0"
  },
  "dependencies": {
    "@slardar/sdk-template": "1.5.1",
    "@slardar/sdk-web": "1.13.0",
    "@slardar/web": "1.12.3",
    "tslib": "^2.3.1"
  },
  "browserslist": [
    "ie >= 9",
    "Edge >= 12",
    "Firefox >= 4",
    "Opera >= 12.1",
    "and_qq >= 10.4",
    "Safari >= 5",
    "iOS >= 3.2",
    "Chrome >= 4",
    "Android >= 37",
    "ChromeAndroid >= 89",
    "UCAndroid >= 12.12"
  ],
  "ava": {
    "extensions": ["ts", "tsx"],
    "require": ["../../tools/ava.setup"],
    "environmentVariables": {
      "TS_NODE_PROJECT": "../../tsconfigs/tsconfig.cjs.json"
    },
    "files": ["src/**/*.spec.{ts,tsx}"]
  },
  "scripts": {
    "test": "ava",
    "cover": "c8 -c='../../tools/sub-c8rc.json' ava",
    "esm": "ttsc -b ./tsconfig.json",
    "build": "batch-rollup -d ./rollup"
  }
}
