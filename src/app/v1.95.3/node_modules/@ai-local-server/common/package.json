{
  "name": "@ai-local-server/common",
  "version": "0.2.11",
  "main": "dist/index.js",
  "browser": "dist/index.bundle.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@ies/starling-cli": "^3.7.13",
    "@pollyjs/adapter-node-http": "^6.0.6",
    "@pollyjs/core": "^6.0.6",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/chai": "^4.3.19",
    "@types/lodash": "^4.17.7",
    "@types/node": "~20.11.0",
    "@types/sinon": "^17.0.3",
    "@types/vscode": "^1.74.0",
    "ajv": "^8.17.1",
    "c8": "^9.1.0",
    "chai": "^5.1.1",
    "concurrently": "^8.2.2",
    "enhanced-resolve": "^5.17.1",
    "esbuild": "^0.23.0",
    "mocha": "~10.2.0",
    "rollup": "^4.19.2",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "sinon": "^17.0.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tslib": "^2.6.3",
    "typescript": "~5.3.3"
  },
  "dependencies": {
    "@artus/injection": "^0.5.3",
    "@byted-icube/devtools-base": "^0.0.48",
    "@byted-icube/devtools-i18n": "0.0.10-alpha.6",
    "axios": "^1.7.3",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.2.1"
  },
  "scripts": {
    "build": "npm run clean && npm run build:bundle && npm run build:package",
    "clean": "rm -rf dist",
    "build:package": "tsc -p ./tsconfig.prod.json && tsc-alias -p ./tsconfig.prod.json",
    "build:bundle": "rollup -c ./rollup.config.js --bundleConfigAsCjs && tsc-alias -p ./tsconfig.prod.json",
    "watch": "concurrently \"npm run watch:package\" \"npm run watch:bundle\"",
    "watch:package": "concurrently \"tsc --watch -p ./tsconfig.prod.json\" \"tsc-alias -w -p ./tsconfig.prod.json\"",
    "watch:bundle": "rollup -c ./rollup.config.js --bundleConfigAsCjs --watch",
    "prepublish": "npm run starling:download && npm run build",
    "test": "mocha --require ts-node/register 'test/**/*.test.ts'",
    "starling:download": "starling download",
    "test-cov": "c8 npm run test"
  }
}