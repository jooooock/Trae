{
  "name": "@slardar/web",
  "version": "1.14.1",
  "description": "Slardar Web SDK",
  "main": "cn/index.cjs.js",
  "module": "cn/index.esm.js",
  "types": "esm/index.d.ts",
  "files": ["cn", "esm", "maliva", "oci", "sg", "euttp", "CHANGELOG.md"],
  "publishConfig": {
    "registry": "http://bnpm.byted.org/"
  },
  "browserslist": [
    "ie >= 9",
    "Edge >= 12",
    "Firefox >= 4",
    "Opera >= 12.1",
    "and_qq >= 10.4",
    "Safari >= 5",
    "iOS >= 3.2",
    "Chrome >= 4",
    "Android >= 37",
    "ChromeAndroid >= 89",
    "UCAndroid >= 12.12"
  ],
  "devDependencies": {
    "@slardar/rollup": "0.0.3"
  },
  "dependencies": {
    "@slardar/sdk-template": "1.5.2",
    "@slardar/sdk-web": "1.17.1"
  },
  "ava": {
    "extensions": ["ts", "tsx"],
    "require": ["../../tools/ava.setup"],
    "environmentVariables": {
      "TS_NODE_PROJECT": "../../tsconfigs/tsconfig.cjs.json"
    },
    "files": ["src/**/*.spec.{ts,tsx}"]
  },
  "scripts": {
    "test": "ava",
    "esm": "ttsc -b ./tsconfig.json",
    "build": "batch-rollup -d ./rollup",
    "cover": "c8 -c='../../tools/sub-c8rc.json' ava",
    "build-local": "SIMPLE_BUILD=true NODE_ENV=development batch-rollup -d ./rollup -f cn",
    "analyze": "library-analyze -p ./cn/*[^0-9].cn.js,./cn/plugins/*.js",
    "uglifySnippet": "uglifyjs snippet.js  -m -o snippet.min.js"
  }
}
