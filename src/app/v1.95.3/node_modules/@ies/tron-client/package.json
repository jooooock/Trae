{
  "name": "@ies/tron-client",
  "version": "1.2.0",
  "description": "tron client sdk",
  "license": "ISC",
  "main": "dist/index.js",
  "bin": {
    "tron-client": "dist/core/modifier.js"
  },
  "files": ["dist", "README.md"],
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "publishConfig": {
    "registry": "https://bnpm.byted.org"
  },
  "repository": {
    "type": "git",
    "url": "git@code.byted.org:ies/tron-util.git"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "npm run clear && tsc && node scripts/index.js",
    "dev": "tsc -w",
    "clear": "rimraf dist",
    "increase": "npm version patch",
    "increase:alpha": "npm version prerelease --preid=alpha",
    "deploy": "npm run build && npm run increase && npm publish",
    "deploy:alpha": "npm run build && npm run increase:alpha && npm publish --tag alpha"
  },
  "dependencies": {
    "@dp/tea-sdk-node": "^3.0.8",
    "@logsdk/node-plugin-http": "^3.4.0",
    "axios": "^0.21.1",
    "chalk": "^4.1.0",
    "lodash": "^4.17.20",
    "semver": "^7.6.2",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/lodash": "^4.14.161",
    "@types/node": "^14.6.1",
    "@types/semver": "^7.5.8",
    "electron-builder": "^22.9.1",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.6",
    "typescript": "^4.8.2"
  }
}
