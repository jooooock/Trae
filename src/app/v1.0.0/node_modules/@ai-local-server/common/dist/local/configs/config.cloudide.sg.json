    "aiSlashCommands": [
        {
            "command": "/Explain",
            "defaultIntent": "explain_code",
            "locations": ["InlineChat", "SideChat"],
            "isDetectNeed": true,
            "isShowInCommandList": true,
            "description": "Explain how the selected code works"
        },
        {
            "command": "/Doc",
            "defaultIntent": "function_comment",
            "locations": ["InlineChat", "SideChat"],
            "isDetectNeed": true,
            "isShowInCommandList": true,
            "description": "Add comments for the selected code"
        },
        {
            "command": "/Fix",
            "defaultIntent": "fix",
            "locations": ["InlineChat", "SideChat"],
            "isDetectNeed": true,
            "isShowInCommandList": true,
            "description": "Fix the problems in the selected code"
        },
        {
            "command": "/Test",
            "defaultIntent": "generate_unittest",
            "locations": ["InlineChat", "SideChat"],
            "isDetectNeed": true,
            "isShowInCommandList": true,
            "description": "Add unit tests code for the selected code"
        }
    ],
