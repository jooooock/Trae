{
  "name": "@slardar/sdk-template",
  "version": "1.5.2",
  "description": "Slardar SDK Template",
  "main": "dist/index.cjs.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "sideEffects": false,
  "files": ["dist", "esm", "CHANGELOG.md"],
  "publishConfig": {
    "registry": "http://bnpm.byted.org/"
  },
  "ava": {
    "extensions": ["ts", "tsx"],
    "require": ["../../tools/ava.setup"],
    "environmentVariables": {
      "TS_NODE_PROJECT": "../../tsconfigs/tsconfig.cjs.json"
    },
    "files": ["src/**/*.spec.{ts,tsx}"]
  },
  "browserslist": [
    "ie >= 9",
    "Edge >= 12",
    "Firefox >= 4",
    "Opera >= 12.1",
    "and_qq >= 10.4",
    "Safari >= 5",
    "iOS >= 3.2",
    "Chrome >= 4",
    "Android >= 37",
    "ChromeAndroid >= 89",
    "UCAndroid >= 12.12"
  ],
  "devDependencies": {
    "@slardar/rollup": "0.0.3"
  },
  "scripts": {
    "test": "ava",
    "esm": "ttsc -b ./tsconfig.json",
    "cover": "c8 -c='../../tools/sub-c8rc.json' ava",
    "build": "batch-rollup -d ./rollup"
  }
}
