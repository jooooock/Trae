{
  "name": "@slardar/sdk-web",
  "version": "1.17.1",
  "description": "Slardar Web SDK",
  "main": "dist/index.cjs.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "files": ["dist", "esm", "CHANGELOG.md"],
  "publishConfig": {
    "registry": "http://bnpm.byted.org/"
  },
  "dependencies": {
    "@slardar/sdk-template": "1.5.2"
  },
  "devDependencies": {
    "@slardar/rollup": "0.0.3"
  },
  "ava": {
    "extensions": ["ts", "tsx"],
    "require": ["../../tools/ava.setup"],
    "environmentVariables": {
      "TS_NODE_PROJECT": "../../tsconfigs/tsconfig.cjs.json"
    },
    "files": ["src/**/*.spec.{ts,tsx}"]
  },
  "browserslist": [
    "ie >= 9",
    "Edge >= 12",
    "Firefox >= 4",
    "Opera >= 12.1",
    "and_qq >= 10.4",
    "Safari >= 5",
    "iOS >= 3.2",
    "Chrome >= 4",
    "Android >= 37",
    "ChromeAndroid >= 89",
    "UCAndroid >= 12.12"
  ],
  "scripts": {
    "test": "ava",
    "esm": "ttsc -b ./tsconfig.json",
    "build": "batch-rollup -d ./rollup",
    "rb": "shx rm -rf ./{dist,esm,tsconfig.tsbuildinfo}  && run-s esm build",
    "cover": "c8 -c='../../tools/sub-c8rc.json' ava",
    "genTypeDesc": "npx esno ./script/generate-type-desc.ts",
    "testHelper": "ava ./src/utils/__tests__/storage.spec.ts",
    "dtsGen:thrift:collector": "dtsGen --int64 number -r ../../../api_thrift/browser/collector_v2 -o src/types/thrift/browser/collector_v2 *.thrift",
    "dtsGen:thrift:settings": "dtsGen --int64 number -r ../../../api_thrift/browser/sdk_settings_v2 -o src/types/thrift/browser/sdk_settings_v2 *.thrift"
  }
}
